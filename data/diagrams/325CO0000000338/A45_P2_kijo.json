  "text_raw": "圧縮力を負担する筋かいは、厚さ三センチメートル以上で幅九センチメートル以上の木材又はこれと同等以上に圧縮力を負担することができる材料として国土交通大臣が定めたもの若しくは国土交通大臣の認定を受けたものを使用したものとしなければならない。",
  "compliance_logic": {
    "scope_condition": {
      "operator": "EXISTS",
      "conditions": [
        {
          "type": "element",
          "value": "圧縮力を負担する筋かい"
        }
      ],
      "note": "建築物に圧縮力を負担する筋かいが存在する場合に適用"
    },
    "judgment_rule": {
      "operator": "OR",
      "conditions": [
        {
          "type": "wood_spec",
          "operator": "AND",
          "conditions": [
            {
              "property": "厚さ",
              "operator": ">=",
              "value": 3,
              "unit": "cm"
            },
            {
              "property": "幅",
              "operator": ">=",
              "value": 9,
              "unit": "cm"
            }
          ]
        },
        {
          "type": "minister_designation",
          "value": "国土交通大臣が定めた同等以上の材料"
        },
        {
          "type": "minister_approval",
          "value": "国土交通大臣の認定を受けた同等以上の材料"
        }
      ]
    },
    "exceptions": null
  },
