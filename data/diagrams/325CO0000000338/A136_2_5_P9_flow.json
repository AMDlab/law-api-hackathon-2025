      "conditions": [
        {
          "type": "location",
          "value": "沿道地区計画の区域内"
        },
        {
          "type": "regulation",
          "value": "法第68条の2第1項に基づく条例で第14号・第15号又は第3項の制限を定める場合（第8項の場合）"
        }
      ],
      "note": "令第136条の2の5第8項に基づき条例を定める場合において、適用除外規定を定める必要がある"
    },
    "judgment_rule": {
      "operator": "AND",
      "conditions": [
        {
          "type": "comparison",
          "description": "敷地の地盤面が沿道整備道路の路面の中心より低い"
        },
        {
          "type": "comparison",
          "description": "第3項の高さの最低限度5m制限を適用した結果、建築物の高さが2階建ての通常の高さを超える"
        }
      ]
    },
