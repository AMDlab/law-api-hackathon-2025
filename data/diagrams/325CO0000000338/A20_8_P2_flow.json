      ],
      "note": "第1項の換気設備基準が適用される居室を有する建築物"
    },
    "judgment_rule": {
      "operator": "OR",
      "conditions": [
        {
          "type": "requirement",
          "value": "国土交通大臣が定めた構造方法の使用",
          "description": "第1項基準と同等以上のホルムアルデヒド対策換気を確保できる構造方法"
        },
        {
          "type": "requirement",
          "value": "国土交通大臣の認定",
          "description": "第1項基準と同等以上のホルムアルデヒド対策換気を確保できることの認定"
        }
      ]
    },
    "exceptions": null
  },
