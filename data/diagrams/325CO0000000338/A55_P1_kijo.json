      "operator": "AND",
      "conditions": [
        {
          "type": "structure",
          "value": "組積造"
        }
      ],
      "note": "組積造の建築物が対象"
    },
    "judgment_rule": {
      "operator": "FORALL",
      "conditions": [
        {
          "ref": "info-wall-thickness-compliance",
          "operator": "==",
          "value": true,
          "note": "各壁の厚さが基準値以上"
        }
      ]
    }
  },
