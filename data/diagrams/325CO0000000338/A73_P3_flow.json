      },
      {
        "id": "proc-identify-rebar",
        "type": "process",
        "title": "はりの引張鉄筋を特定",
        "process_type": "undefined_input",
        "description": "各はりに属する引張鉄筋を特定する"
      },
      {
        "id": "dec-has-rebar",
        "type": "decision",
        "title": "判定対象の引張鉄筋があるか？",
        "decision_type": "binary",
        "description": "判定対象となる未判定の引張鉄筋が残っているかを確認"
      },
      {
        "id": "dec-welded",
        "type": "decision",
        "title": "柱の主筋に溶接しているか？",
        "decision_type": "binary",
        "description": "引張鉄筋が柱の主筋に溶接されているかを確認",
        "condition": {
          "operator": "EQ",
          "lhs": { "var": "W", "desc": "主筋への溶接有無" },
          "rhs": { "value": true }
        }
      },
      {
        "id": "proc-welded-exempt",
        "type": "process",
        "title": "溶接による適用除外",
        "process_type": "mechanical",
        "description": "当該鉄筋は柱の主筋に溶接されているため、定着長さ基準の適用除外"
      },
      {
        "id": "proc-read-diameter",
        "type": "process",
        "title": "鉄筋径（D）を読み取り",
        "process_type": "undefined_input",
        "description": "引張鉄筋の径を読み取る"
      },
      {
        "id": "proc-read-anchorage",
        "type": "process",
        "title": "定着長さ（L）を読み取り",
        "process_type": "undefined_input",
        "description": "柱に定着される部分の長さを読み取る"
      },
      {
        "id": "proc-calc-required",
        "type": "process",
        "title": "必要定着長さ（Lreq = D×40）を算出",
        "process_type": "mechanical",
        "description": "径の40倍の値を算出して必要定着長さとする"
      },
      {
        "id": "dec-length-compare",
        "type": "decision",
        "title": "定着長さ L >= 必要定着長さ Lreq か？",
        "decision_type": "binary",
        "description": "定着長さが径の40倍以上かを判定",
        "condition": {
          "operator": "GTE",
          "lhs": { "var": "L", "desc": "定着長さ" },
          "rhs": { "var": "Lreq", "desc": "必要定着長さ（D×40）" }
        }
      },
      {
        "id": "proc-length-ok",
        "type": "process",
        "title": "定着長さ基準適合",
        "process_type": "mechanical",
        "description": "当該鉄筋の定着長さは径の40倍以上であり、本則に適合"
      },
      {
        "id": "dec-calc-safe",
        "type": "decision",
        "title": "構造計算で安全確認済みか？",
        "decision_type": "binary",
        "description": "国土交通大臣が定める基準に従った構造計算によって構造耐力上安全であることが確かめられたか",
        "condition": {
          "operator": "EQ",
          "lhs": { "var": "S", "desc": "構造計算による安全確認" },
          "rhs": { "value": true }
        }
      },
      {
        "id": "proc-calc-exempt",
        "type": "process",
        "title": "構造計算によるただし書き適用",
        "process_type": "human_judgment",
        "description": "構造計算により安全が確認されたため、ただし書き規定により適合"
      },
      {
        "id": "proc-rebar-fail",
        "type": "process",
        "title": "当該鉄筋は不適合",
        "process_type": "mechanical",
        "description": "当該引張鉄筋は定着長さ基準を満たさず、かつ構造計算による安全確認もされていない"
      },
      {
        "id": "terminal-fail",
        "type": "terminal",
        "title": "不適合",
        "result": "fail",
        "description": "令73条3項に不適合（定着長さ基準を満たさない引張鉄筋が存在）"
      },
      {
        "id": "terminal-pass",
        "type": "terminal",
        "title": "適合",
        "result": "pass",
        "description": "令73条3項に適合（全ての引張鉄筋が溶接・定着長さ基準・構造計算のいずれかで適合）"
      }
    ],
