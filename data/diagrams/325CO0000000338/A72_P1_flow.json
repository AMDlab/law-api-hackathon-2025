      "conditions": [
        {
          "item": "第1号",
          "description": "有害物質を含まないこと"
        },
        {
          "item": "第2号",
          "description": "骨材が適切な大きさであること"
        },
        {
          "item": "第3号",
          "description": "骨材が適切な粒度・粒形で必要な性能を有すること"
        }
      ]
    }
  },
