      "operator": "AND",
      "conditions": [
        {
          "type": "building_type",
          "value": "法第3条第2項の規定により法第25条の規定の適用を受けない木造建築物等"
        },
        {
          "type": "work_type",
          "value": "増築又は改築"
        }
      ],
      "note": "大規模木造建築物等の屋根・外壁に関する既存不適格建築物における増築・改築が対象"
    },
    "judgment_rule": {
      "operator": "AND",
      "conditions": [
        {
          "ref": "info-floor-area-check",
          "description": "工事着手が基準時以後の増改築部分の対象床面積合計が50m2以下"
        }
      ]
    }
  },
