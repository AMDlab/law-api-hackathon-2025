        }
      ],
      "note": "法別表第二（へ）項第四号の適用を受ける建築物に附属する自動車車庫"
    },
    "judgment_rule": {
      "operator": "OR",
      "conditions": [
        {
          "id": "item1",
          "description": "敷地内基準適合（第一号）",
          "operator": "AND",
          "conditions": [
            {
              "operator": "<=",
              "lhs": "自動車車庫床面積+工作物築造面積",
              "rhs": "敷地内建築物延べ面積（車庫部分除く）"
            },
            {
              "operator": "NOT",
              "condition": "三階以上を自動車車庫に供する"
            }
          ]
        },
        {
          "id": "item2",
          "description": "公告対象区域内基準適合（第二号）",
          "operator": "AND",
          "conditions": [
            {
              "operator": "<=",
              "lhs": "自動車車庫床面積+他車庫床面積+工作物築造面積",
              "rhs": "公告対象区域内建築物延べ面積（車庫部分除く）"
            },
            {
              "operator": "NOT",
              "condition": "三階以上を自動車車庫に供する"
            }
          ]
        }
      ]
    }
  },
