        {
          "type": "material",
          "value": "コンクリートを使用する建築物"
        }
      ],
      "note": "コンクリートを構造材料として使用する場合に適用"
    },
    "judgment_rule": {
      "operator": "AND",
      "conditions": [
        {
          "ref": "info-allowable-stress-compliance",
          "operator": "==",
          "value": true
