  "text_raw": "建築物の地下部分の各部分に作用する地震力は、当該部分の固定荷重と積載荷重との和に次の式に適合する水平震度を乗じて計算しなければならない。ただし、地震時における建築物の振動の性状を適切に評価して計算をすることができる場合においては、当該計算によることができる。",
  "compliance_logic": {
    "scope_condition": {
      "operator": "AND",
      "conditions": [
        {
          "type": "building_element",
          "value": "建築物の地下部分を有するもの"
        }
      ],
      "note": "地下部分を有する建築物に適用"
    },
    "judgment_rule": {
      "operator": "AND",
      "conditions": [
        {
          "type": "calculation",
          "value": "地震力 = (固定荷重 + 積載荷重) × 水平震度",
          "description": "地下部分の各部分に作用する地震力の計算式"
        },
        {
          "type": "formula",
          "value": "k >= 0.1 × (1 - (H/40)) × Z",
          "description": "水平震度の適合式"
        }
      ]
    },
    "exceptions": {
      "operator": "OR",
      "conditions": [
        {
          "type": "calculation_method",
          "value": "地震時における建築物の振動の性状を適切に評価して計算をすることができる場合"
        }
      ],
      "effect": "ALTERNATIVE",
      "description": "適切な振動評価計算ができる場合は、当該計算によることができる"
    }
  },
