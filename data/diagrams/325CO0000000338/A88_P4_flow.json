        "title": "k ≧ k_min か？",
        "decision_type": "binary",
        "description": "適用水平震度が最低水平震度以上か判定",
        "condition": {
          "operator": "GTE",
          "lhs": { "var": "k", "desc": "適用水平震度" },
          "rhs": { "var": "k_min", "desc": "最低水平震度" }
        }
