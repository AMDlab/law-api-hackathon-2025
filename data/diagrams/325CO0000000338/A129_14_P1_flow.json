      "operator": "AND",
      "conditions": [
        {
          "note": "法第33条の適用対象となる建築物（高さ20mを超える建築物）",
          "ref": "法::A33"
        }
      ]
    },
    "judgment_rule": {
      "operator": "AND",
      "conditions": [
        {
          "note": "20m超部分が避雷設備により雷撃から保護されている",
          "ref": "info-protected-from-lightning",
          "operator": "==",
          "value": true
        }
      ]
    }
  },
