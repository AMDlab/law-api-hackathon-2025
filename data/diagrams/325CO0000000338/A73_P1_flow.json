  "text_raw": "鉄筋の末端は、かぎ状に折り曲げて、コンクリートから抜け出ないように定着しなければならない。ただし、次の各号に掲げる部分以外の部分に使用する異形鉄筋にあつては、その末端を折り曲げないことができる。一 柱及びはり（基礎ばりを除く。）の出すみ部分 二 煙突",
  "compliance_logic": {
    "scope_condition": {
      "operator": "AND",
      "conditions": [
        {
          "type": "structure",
          "value": "鉄筋コンクリート造"
        }
      ],
      "note": "鉄筋コンクリート造の建築物における鉄筋"
    },
    "judgment_rule": {
      "operator": "OR",
      "conditions": [
        {
          "note": "本文適用: かぎ状に折り曲げて定着",
          "ref": "info-hook-anchored",
          "operator": "==",
          "value": true
        },
        {
          "note": "ただし書き適用: 異形鉄筋で指定部位以外",
          "operator": "AND",
          "conditions": [
            {
              "ref": "info-deformed-bar",
              "operator": "==",
              "value": true
            },
            {
              "ref": "info-specified-location",
              "operator": "==",
              "value": false
            }
          ]
        }
      ]
    },
    "exceptions": {
      "operator": "AND",
      "conditions": [
        {
          "note": "異形鉄筋であること",
          "ref": "info-deformed-bar",
          "operator": "==",
          "value": true
        },
        {
          "note": "各号に掲げる部分以外の部分であること",
          "ref": "info-specified-location",
          "operator": "==",
          "value": false
        }
      ],
      "effect": "折り曲げ省略可"
    }
  },
