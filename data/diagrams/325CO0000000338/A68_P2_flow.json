  "text_raw": "高力ボルト孔の径は、高力ボルトの径より二ミリメートルを超えて大きくしてはならない。ただし、高力ボルトの径が二十七ミリメートル以上であり、かつ、構造耐力上支障がない場合においては、高力ボルト孔の径を高力ボルトの径より三ミリメートルまで大きくすることができる。",
  "compliance_logic": {
    "scope_condition": {
      "operator": "AND",
      "conditions": [
        {
          "type": "existence",
          "value": "高力ボルト接合を用いる建築物"
        }
      ],
      "note": "高力ボルト接合を有する建築物に適用"
    },
    "judgment_rule": {
      "operator": "OR",
      "conditions": [
        {
          "note": "原則: 高力ボルト孔の径≦高力ボルトの径+2mm",
          "operator": "<=",
          "lhs": { "ref": "info-hole-diameter" },
          "rhs": { "operator": "+", "lhs": { "ref": "info-bolt-diameter" }, "rhs": { "value": 2 } }
        },
        {
          "note": "ただし書き: 高力ボルトの径≧27mm かつ 構造耐力上支障がない場合は、高力ボルト孔の径≦高力ボルトの径+3mm",
          "operator": "AND",
          "conditions": [
            { "ref": "info-bolt-diameter", "operator": ">=", "value": 27 },
            { "ref": "info-structural-safety", "operator": "==", "value": true },
            { "ref": "info-hole-diameter", "operator": "<=", "rhs": { "operator": "+", "lhs": { "ref": "info-bolt-diameter" }, "rhs": { "value": 3 } } }
          ]
        }
      ]
    },
    "exceptions": null
  },
  "flow_diagram": {
