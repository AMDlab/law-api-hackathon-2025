              "related_article": "令::A112:P1:I2",
              "keywords": ["階段室", "昇降機", "昇降路", "便所", "公衆電話所"],
              "sub_conditions": [
                { "id": "sub-2a", "desc": "一時間準耐火基準に適合する準耐火構造の床・壁で区画", "operator": "OR" },
                { "id": "sub-2b", "desc": "特定防火設備で区画", "operator": "OR" }
              ],
              "note": "竪穴区画として別途区画されているもの"
