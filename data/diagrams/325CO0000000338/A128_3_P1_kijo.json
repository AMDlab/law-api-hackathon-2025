    },
    "judgment_rule": {
      "operator": "OR",
      "conditions": [
        {
          "operator": "AND",
          "conditions": [
            { "type": "passageway_compliance", "description": "地下道が第1号〜第6号の基準に該当" },
            { "type": "comparison", "operator": ">=", "lhs": "接する長さ", "rhs": "2m" }
          ]
        },
        {
          "type": "exception",
          "description": "公衆便所、公衆電話所その他これらに類するもの",
          "effect": "接する長さを2m未満とすることができる"
        }
      ]
    },
    "exceptions": {
