        "condition": {
          "operator": "GTE",
          "lhs": { "var": "cover_depth", "desc": "かぶり厚さ" },
          "rhs": { "value": 3, "unit": "cm" }
        }
      },
      {
        "id": "dec-cat3-check",
        "type": "decision",
        "title": "かぶり厚さ≧4cmか？",
        "decision_type": "binary",
        "description": "直接土に接する壁、柱、床、はり又は布基礎の立上り部分のかぶり厚さが4cm以上か判定",
        "condition": {
          "operator": "GTE",
          "lhs": { "var": "cover_depth", "desc": "かぶり厚さ" },
          "rhs": { "value": 4, "unit": "cm" }
        }
      },
      {
        "id": "dec-cat4-check",
        "type": "decision",
        "title": "かぶり厚さ≧6cmか？（捨コン除く）",
        "decision_type": "binary",
        "description": "基礎（布基礎の立上り部分を除く）のかぶり厚さが捨コンクリート部分を除いて6cm以上か判定",
        "condition": {
          "operator": "GTE",
          "lhs": { "var": "cover_depth", "desc": "かぶり厚さ（捨コン除く）" },
          "rhs": { "value": 6, "unit": "cm" }
        }
      },
