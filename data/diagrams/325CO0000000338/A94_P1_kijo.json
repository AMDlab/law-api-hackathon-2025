      "conditions": [
        {
          "type": "reference",
          "value": "令第89条から第93条に該当しない材料"
        }
      ],
      "note": "第89条から第93条に定めるもののほかの材料に適用"
    },
    "judgment_rule": {
      "operator": "AND",
      "conditions": [
        {
          "ref": "info-long-term-compliance",
          "operator": "==",
          "value": true
        },
        {
          "ref": "info-short-term-compliance",
          "operator": "==",
          "value": true
        }
      ]
    },
    "exceptions": null
  },
