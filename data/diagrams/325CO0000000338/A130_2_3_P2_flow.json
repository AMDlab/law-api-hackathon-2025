  "text_raw": "特定行政庁が法第五十一条ただし書の規定による許可をする場合において、前項第四号から第六号までに規定する規模の範囲内において、増築し、又は用途を変更することができる規模を定めたときは、同項の規定にかかわらず、その規模を同条ただし書の規定により政令で定める規模とする。",
  "compliance_logic": {
    "scope_condition": {
      "operator": "AND",
      "conditions": [
        {
          "type": "permit_status",
          "value": "法第51条ただし書の許可対象案件"
        },
        {
          "type": "building_use",
          "value": "卸売市場、と畜場、火葬場、汚物処理場、ごみ処理施設、産業廃棄物処理施設"
        }
      ],
      "note": "特定行政庁が法51条ただし書の規定による許可をする場合に適用"
    },
    "judgment_rule": {
      "operator": "IF_THEN",
      "conditions": [
        {
          "type": "administrative_decision",
          "value": "特定行政庁が規模を定めた場合",
          "description": "前項第4号から第6号までに規定する規模の範囲内で、増築又は用途変更できる規模を特定行政庁が定めた場合"
        }
      ],
      "effect": "その規模を政令で定める規模とする"
    }
  },
