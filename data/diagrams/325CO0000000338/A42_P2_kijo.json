  "text_raw": "土台は、基礎に緊結しなければならない。ただし、平家建ての建築物で延べ面積が五十平方メートル以内のものについては、この限りでない。",
  "compliance_logic": {
    "scope_condition": {
      "note": "全ての建築物に適用（土台を有する場合）"
    },
    "judgment_rule": {
      "operator": "OR",
      "conditions": [
        {
          "ref": "info-exempt",
          "operator": "==",
          "value": true,
          "note": "ただし書き適用除外に該当"
        },
        {
          "ref": "info-fastening-compliance",
          "operator": "==",
          "value": true,
          "note": "土台が基礎に緊結されている"
        }
      ]
    },
    "exceptions": {
      "operator": "AND",
      "conditions": [
        {
          "type": "building_structure",
          "value": "平家建て"
        },
        {
          "type": "numeric",
          "property": "延べ面積",
          "operator": "<=",
          "value": 50,
          "unit": "m2"
        }
      ],
      "effect": "EXEMPT"
    }
  },
