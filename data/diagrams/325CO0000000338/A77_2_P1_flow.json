        "related_articles": ["令第82条第4号"]
      },
      {
        "id": "proc-exempt-apply",
        "type": "process",
        "title": "ただし書き適用",
        "process_type": "mechanical",
        "description": "構造計算による確認がなされたため、本条の適用除外となる"
      },
      {
        "id": "proc-identify-thickness",
        "type": "process",
        "title": "床版厚さ・有効張り間長さの特定",
        "process_type": "undefined_input",
        "description": "各床版の厚さ及び短辺方向における有効張り間長さを特定する"
      },
      {
        "id": "dec-thickness-min",
        "type": "decision",
        "title": "厚さ >= 8cm か？",
        "decision_type": "binary",
        "description": "床版の厚さが8センチメートル以上であるか判定",
        "condition": {
          "operator": "GTE",
          "lhs": { "var": "T", "desc": "床版の厚さ" },
          "rhs": { "value": 8, "unit": "cm" }
        }
      },
      {
        "id": "dec-thickness-span",
        "type": "decision",
        "title": "厚さ >= 有効張り間長さの1/40 か？",
        "decision_type": "binary",
        "description": "床版の厚さが短辺方向における有効張り間長さの40分の1以上であるか判定",
        "condition": {
          "operator": "GTE",
          "lhs": { "var": "T", "desc": "床版の厚さ" },
          "rhs": { "var": "L/40", "desc": "有効張り間長さの1/40" }
        }
      },
      {
        "id": "proc-item1-pass",
        "type": "process",
        "title": "第1号基準適合",
