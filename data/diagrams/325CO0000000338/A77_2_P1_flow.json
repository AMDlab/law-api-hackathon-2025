      },
      {
        "id": "dec-rebar-short",
        "type": "decision",
        "title": "短辺方向の引張鉄筋間隔 <= 20cm か？",
        "decision_type": "binary",
        "description": "短辺方向における引張鉄筋の間隔が20センチメートル以下であるか判定",
        "condition": {
          "operator": "LTE",
          "lhs": { "var": "RS", "desc": "短辺方向の引張鉄筋間隔" },
          "rhs": { "value": 20, "unit": "cm" }
        }
      },
      {
        "id": "dec-rebar-long",
        "type": "decision",
        "title": "長辺方向の引張鉄筋間隔 <= 30cm か？",
        "decision_type": "binary",
        "description": "長辺方向における引張鉄筋の間隔が30センチメートル以下であるか判定",
        "condition": {
          "operator": "LTE",
          "lhs": { "var": "RL", "desc": "長辺方向の引張鉄筋間隔" },
          "rhs": { "value": 30, "unit": "cm" }
        }
      },
      {
        "id": "dec-rebar-thickness-short",
        "type": "decision",
        "title": "短辺方向の引張鉄筋間隔 <= 床版厚さの3倍 か？",
        "decision_type": "binary",
        "description": "短辺方向における引張鉄筋の間隔が床版厚さの3倍以下であるか判定",
        "condition": {
          "operator": "LTE",
          "lhs": { "var": "RS", "desc": "短辺方向の引張鉄筋間隔" },
          "rhs": { "var": "3*T", "desc": "床版厚さの3倍" }
        }
      },
      {
        "id": "dec-rebar-thickness-long",
        "type": "decision",
        "title": "長辺方向の引張鉄筋間隔 <= 床版厚さの3倍 か？",
        "decision_type": "binary",
        "description": "長辺方向における引張鉄筋の間隔が床版厚さの3倍以下であるか判定",
        "condition": {
          "operator": "LTE",
          "lhs": { "var": "RL", "desc": "長辺方向の引張鉄筋間隔" },
          "rhs": { "var": "3*T", "desc": "床版厚さの3倍" }
        }
      },
      {
        "id": "proc-item2-pass",
        "type": "process",
        "title": "第2号基準適合",
        "process_type": "mechanical",
        "description": "当該床版は第2号の引張鉄筋間隔要件に適合"
      },
      {
        "id": "dec-all-slabs-checked",
        "type": "decision",
        "title": "全ての床版を確認したか？",
        "decision_type": "binary",
        "description": "構造耐力上主要な部分である全ての床版について確認が完了したか判定"
      },
      {
        "id": "proc-next-slab",
        "type": "process",
        "title": "次の床版を確認",
        "process_type": "mechanical",
        "description": "次の床版について第1号・第2号の確認を行う"
      },
      {
