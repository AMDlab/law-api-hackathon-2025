      "operator": "AND",
      "conditions": [
        {
          "type": "structure_type",
          "value": "補強コンクリートブロック造"
        }
      ],
      "note": "補強コンクリートブロック造の建築物に適用"
    },
    "judgment_rule": {
      "operator": "AND",
      "conditions": [
        {
          "type": "numeric",
          "value": "耐力壁の厚さ >= 15cm",
          "description": "厚さが15センチメートル以上"
        },
        {
          "type": "numeric",
          "value": "耐力壁の厚さ >= 支点間距離 / 50",
          "description": "水平力に対する支点間距離の1/50以上"
        }
      ]
    },
    "exceptions": null
