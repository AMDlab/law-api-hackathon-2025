        }
      ],
      "note": "物品販売業を営む店舗が対象"
    },
    "judgment_rule": {
      "operator": "GTE",
      "lhs": {
        "description": "避難階に設ける屋外への出口の幅の合計"
      },
      "rhs": {
        "description": "床面積が最大の階における床面積(m2) x 0.6(m) / 100(m2)"
      }
    },
    "exceptions": null
  },
