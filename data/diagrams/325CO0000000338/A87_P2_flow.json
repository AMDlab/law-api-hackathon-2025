      "conditions": [
        {
          "type": "reference",
          "value": "令第87条第1項の風圧力算出"
        }
      ],
      "note": "風圧力を算出する必要がある建築物に適用"
    },
    "judgment_rule": {
      "operator": "EQ",
      "lhs": {
        "ref": "info-velocity-pressure",
        "description": "算出された速度圧"
