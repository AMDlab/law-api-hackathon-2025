  "text_raw": "法第三条第二項の規定により法第二十条の規定の適用を受けない建築物について法第八十六条の七第一項の規定により政令で定める範囲は、増築及び改築については、次の各号に掲げる範囲とし、同項の政令で定める基準は、それぞれ当該各号に定める基準とする。",
  "compliance_logic": {
    "scope_condition": {
      "operator": "AND",
      "conditions": [
        {
          "type": "building_type",
          "value": "法第3条第2項の規定により法第20条の規定の適用を受けない建築物"
        },
        {
          "type": "work_type",
          "value": "増築又は改築"
        }
      ],
      "note": "構造耐力に関する既存不適格建築物の増築・改築が対象"
    },
    "judgment_rule": {
      "operator": "OR",
      "conditions": [
        {
          "description": "第1号: 増築又は改築の全て（第2号・第3号を除く）で所定の構造方法に適合",
          "ref": "info-item1-compliance"
        },
        {
          "description": "第2号: 床面積1/20超〜1/2以下で所定の構造方法に適合",
          "ref": "info-item2-compliance"
        },
        {
          "description": "第3号: 床面積1/20以下で所定の構造方法に適合",
          "ref": "info-item3-compliance"
        }
      ]
    }
  },
