      "operator": "OR",
      "conditions": [
        {
          "type": "building_structure",
          "value": "主要構造部の全部が木造（法第2条第9号の2イ適合を除く）かつ延べ面積1000㎡超"
        },
        {
          "type": "building_structure",
          "value": "主要構造部の一部が木造かつ延べ面積（耐火区画部分除外後）1000㎡超"
        }
      ],
      "note": "主要構造部が木造の大規模建築物に適用"
    },
    "judgment_rule": {
      "operator": "AND",
      "conditions": [
        {
          "type": "requirement",
          "value": "避難上及び消火上有効な通路の設置",
          "description": "国土交通大臣が定める基準に適合する通路を建築物の周囲に設ける"
        }
      ]
    },
    "exceptions": {
