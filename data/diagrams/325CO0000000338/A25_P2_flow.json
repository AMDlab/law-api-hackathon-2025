    "judgment_rule": {
      "operator": "FORALL",
      "conditions": [
        {
          "type": "requirement",
          "value": "両側（手すり設置側を除く）に側壁又はこれに代わるものを設ける",
          "description": "階段及び踊場の手すりが設けられていない側には、側壁又はこれに代わるものを設けなければならない"
        }
      ]
