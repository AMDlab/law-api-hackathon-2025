  "text_raw": "避難階においては、階段から屋外への出口の一に至る歩行距離は第百二十条に規定する数値以下と、居室（避難上有効な開口部を有するものを除く。）の各部分から屋外への出口の一に至る歩行距離は同条に規定する数値の二倍以下としなければならない。",
  "compliance_logic": {
    "scope_condition": {
      "operator": "AND",
      "conditions": [
        { "type": "floor", "value": "避難階" }
      ],
      "note": "避難階に適用"
    },
    "judgment_rule": {
      "operator": "AND",
      "conditions": [
        {
          "description": "階段から屋外への出口の一に至る歩行距離が令第120条に規定する数値以下",
          "lhs": { "ref": "D1", "description": "階段から屋外への出口の一に至る歩行距離" },
          "operator": "<=",
          "rhs": { "ref": "L1", "description": "令第120条に規定する数値" }
        },
        {
          "description": "居室（避難上有効な開口部を有するものを除く）の各部分から屋外への出口の一に至る歩行距離が令第120条に規定する数値の二倍以下",
          "lhs": { "ref": "D2", "description": "居室の各部分から屋外への出口の一に至る歩行距離" },
          "operator": "<=",
          "rhs": { "ref": "L2", "description": "令第120条に規定する数値の二倍" }
        }
      ]
    }
  },
