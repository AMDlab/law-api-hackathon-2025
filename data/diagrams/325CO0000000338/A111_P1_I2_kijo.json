      "operator": "AND",
      "conditions": [
        {"type": "subject", "value": "居室の開口部"}
      ],
      "note": "居室に設けられた窓その他の開口部が対象"
    },
    "judgment_rule": {
      "operator": "AND",
      "conditions": [
        {"ref": "info-direct-air", "operator": "==", "value": true, "note": "直接外気に接する"},
        {"ref": "info-evacuation-effective", "operator": "==", "value": true, "note": "避難上有効な構造"},
        {
          "operator": "OR",
          "conditions": [
            {"ref": "info-circle-ok", "operator": "==", "value": true, "note": "直径1m以上の円が内接可能"},
            {
              "operator": "AND",
              "conditions": [
                {"ref": "info-width-ok", "operator": "==", "value": true, "note": "幅75cm以上"},
                {"ref": "info-height-ok", "operator": "==", "value": true, "note": "高さ1.2m以上"}
              ]
            }
          ]
        }
      ]
    }
  },
