  "text_raw": "法第三十五条の二の規定により政令で定める階数が三以上である建築物は、延べ面積が五百平方メートルを超えるもの（学校等の用途に供するものを除く。）以外のものとする。",
  "compliance_logic": {
    "scope_condition": {
      "operator": "AND",
      "conditions": [
        { "type": "article_scope", "value": "法第35条の2の適用対象（階数3以上の建築物）" }
      ],
      "note": "法第35条の2で定める内装制限のうち、階数3以上の建築物に関する委任規定"
    },
    "judgment_rule": {
      "operator": "AND",
      "conditions": [
        { "ref": "階数", "operator": ">=", "value": 3, "unit": "階" },
        { "ref": "延べ面積", "operator": ">", "value": 500, "unit": "m2" },
        { "ref": "用途", "operator": "NOT_IN", "value": "学校等" }
      ]
    },
    "exceptions": {
      "operator": "OR",
      "conditions": [
        { "type": "use", "value": "学校等の用途", "effect": "EXEMPT" }
      ],
      "effect": "EXEMPT"
    }
  },
