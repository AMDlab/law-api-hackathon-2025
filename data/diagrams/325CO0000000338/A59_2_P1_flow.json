        {
          "type": "building_dimension",
          "value": "軒の高さ9mを超える"
        }
      ],
      "note": "高さ13m超又は軒の高さ9m超の建築物が対象"
    },
    "judgment_rule": {
      "operator": "AND",
      "conditions": [
        {
          "ref": "info-reinforcement-compliance",
          "operator": "==",
          "value": true
        }
      ]
    }
  },
