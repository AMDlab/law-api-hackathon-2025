  "text_raw": "法第六十八条の九第二項の規定に基づく条例で建築物の敷地面積の最低限度を定める場合においては、当該条例に、当該条例の規定の施行又は適用の際、現に建築物の敷地として使用されている土地で当該規定に適合しないもの及び現に存する所有権その他の権利に基づいて建築物の敷地として使用するならば当該規定に適合しないこととなる土地について、その全部を一の敷地として使用する場合の適用の除外に関する規定（法第三条第三項第一号及び第五号の規定に相当する規定を含む。）を定めるものとする。",
  "compliance_logic": {
    "scope_condition": {
      "operator": "AND",
      "conditions": [
        {
          "type": "location",
          "value": "準景観地区内"
        },
        {
          "type": "regulation",
          "value": "条例で敷地面積の最低限度が定められている場合"
        }
      ],
      "note": "法第68条の9第2項に基づく条例により敷地面積の最低限度が定められている準景観地区に適用"
    },
    "judgment_rule": {
      "operator": "OR",
      "conditions": [
        {
          "type": "existing_use",
          "value": "条例施行時に現に敷地として使用",
          "description": "規定の施行又は適用の際、現に建築物の敷地として使用されている土地"
        },
        {
          "type": "existing_right",
          "value": "既存権利に基づく使用予定",
          "description": "現に存する所有権その他の権利に基づいて建築物の敷地として使用するならば規定に適合しないこととなる土地"
        }
      ]
    },
    "exceptions": {
      "operator": "AND",
      "conditions": [
        {
          "type": "use_condition",
          "value": "敷地全部を一の敷地として使用"
        }
      ],
      "effect": "EXEMPT",
      "description": "敷地面積の最低限度規定の適用を除外"
    }
  },
