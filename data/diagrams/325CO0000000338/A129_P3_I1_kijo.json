      "operator": "AND",
      "conditions": [
        {
          "type": "building_standard",
          "value": "主要構造部が準耐火構造又は不燃材料で造られた建築物の階"
        }
      ],
      "note": "階避難安全検証法の適用対象"
    },
    "judgment_rule": {
      "operator": "AND",
      "conditions": [
        {
          "type": "verification",
          "value": "避難時間に基づく検証",
          "description": "居室避難時間≦煙降下時間、かつ階避難時間≦煙流入時間"
        }
      ]
    }
  },
