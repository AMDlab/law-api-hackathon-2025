      "operator": "AND",
      "conditions": [
        {
          "type": "structural",
          "value": "構造耐力上主要な部分である継手又は仕口"
        }
      ],
      "note": "構造耐力上主要な部分である継手又は仕口が対象"
    },
    "judgment_rule": {
      "operator": "AND",
      "conditions": [
        {
          "note": "存在応力を伝えることができる構造",
          "operator": "OR",
          "conditions": [
            {
              "ref": "info-minister-method",
              "operator": "==",
              "value": true
            },
            {
