  "text_raw": "建築物の避難階以外の階が次の各号のいずれかに該当する場合においては、その階から避難階又は地上に通ずる二以上の直通階段を設けなければならない。",
  "compliance_logic": {
    "scope_condition": {
      "operator": "AND",
      "conditions": [
        {
          "type": "building_part",
          "value": "避難階以外の階"
        }
      ],
      "note": "避難階を除くすべての階が対象"
    },
    "judgment_rule": {
      "operator": "OR",
      "conditions": [
        {
          "type": "building_use",
          "value": "一号：劇場、映画館、演芸場、観覧場、公会堂又は集会場の用途に供する階でその階に客席、集会室等を有するもの"
        },
        {
          "type": "building_use",
          "value": "二号：物品販売業を営む店舗（床面積の合計が1500㎡超）の用途に供する階でその階に売場を有するもの"
        },
        {
          "type": "building_use",
          "value": "三号：キャバレー、ナイトクラブ等の用途に供する階でその階に客席等を有するもの（一定の除外条件あり）"
        },
        {
          "type": "building_use",
          "value": "四号：病院・診療所の病室床面積合計又は児童福祉施設等の居室床面積合計が50㎡超の階"
        },
        {
          "type": "building_use",
          "value": "五号：ホテル・旅館等の宿泊室、共同住宅の居室、寄宿舎の寝室の床面積合計が100㎡超の階"
        },
        {
          "type": "building_use",
          "value": "六号イ：6階以上の階で居室を有するもの（一定の除外条件あり）"
        },
        {
          "type": "building_use",
          "value": "六号ロ：5階以下の階で居室床面積合計が避難階直上階200㎡超、その他の階100㎡超"
        }
      ]
    }
  },
