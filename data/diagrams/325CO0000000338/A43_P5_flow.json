  "text_raw": "階数が二以上の建築物における隅柱又はこれに準ずる柱は、通し柱としなければならない。ただし、接合部を通し柱と同等以上の耐力を有するように補強した場合においては、この限りでない。",
  "compliance_logic": {
    "scope_condition": {
      "operator": "AND",
      "conditions": [
        {
          "type": "building",
          "value": "階数が二以上の建築物"
        }
      ],
      "note": "地階を除く階数が2以上の建築物に適用"
    },
    "judgment_rule": {
      "operator": "FOR_ALL",
      "conditions": [
        {
          "ref": "info-compliance",
          "operator": "==",
          "value": true
        }
      ]
    },
    "exceptions": {
      "operator": "AND",
      "conditions": [
        {
          "ref": "info-reinforcement",
          "operator": "==",
          "value": true,
          "note": "接合部を通し柱と同等以上の耐力を有するように補強した場合"
        }
      ],
      "effect": "EXEMPT"
    }
  },
