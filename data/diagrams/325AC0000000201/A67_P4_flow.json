  "text_raw": "第五十三条の二第三項の規定は、前項の都市計画において建築物の敷地面積の最低限度が定められ、又は変更された場合に準用する。この場合において、同条第三項中「第一項」とあるのは、「第六十七条第三項」と読み替えるものとする。",
  "compliance_logic": {
    "scope_condition": {
      "operator": "AND",
      "conditions": [
        {
          "type": "location",
          "value": "特定防災街区整備地区に関する都市計画内"
        },
        {
          "type": "temporal",
          "value": "第3項の敷地面積最低限度が定められ又は変更された際"
        }
      ],
      "note": "特定防災街区整備地区に関する都市計画において敷地面積最低限度が定められ又は変更された区域"
    },
    "judgment_rule": {
      "operator": "AND",
      "conditions": [
        {
          "operator": "OR",
          "conditions": [
            {
              "ref": "info-existing-nonconforming-site",
              "operator": "==",
              "value": true,
              "note": "制定・変更時に既に不適格だった敷地"
            },
            {
              "ref": "info-rights-based-nonconforming",
              "operator": "==",
              "value": true,
              "note": "制定・変更時に所有権等に基づき使用すれば不適格となる土地"
            }
          ]
        },
        {
          "ref": "info-single-site-use",
          "operator": "==",
          "value": true,
          "note": "全部を一の敷地として使用"
        }
      ]
    },
    "exceptions": {
      "operator": "OR",
      "conditions": [
        {
          "ref": "info-previous-violation",
          "operator": "==",
          "value": true,
          "note": "変更前の制限に既に違反していた"
        },
        {
          "ref": "info-became-conforming",
          "operator": "==",
          "value": true,
          "note": "後に適合するに至った"
        }
      ],
      "effect": "NO_EXEMPTION"
    }
  },
