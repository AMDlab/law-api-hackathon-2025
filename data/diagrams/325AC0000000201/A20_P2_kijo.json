      "operator": "AND",
      "conditions": [
        {
          "type": "building_structure",
          "note": "政令で定める部分が二以上ある建築物"
        }
      ],
      "note": "エキスパンションジョイント等で区画された部分が二以上ある場合"
    },
    "judgment_rule": {
      "operator": "AND",
      "conditions": [
        {
          "ref": "info-deemed-parts",
          "operator": "exists",
          "note": "政令で定める部分が特定されている"
        },
        {
          "ref": "info-parts-count",
          "operator": ">=",
          "value": 2,
          "note": "当該部分が二以上"
        },
        {
          "ref": "info-each-compliance",
          "operator": "==",
          "value": true,
          "note": "各部分が法20条1項基準に適合"
        }
      ]
    }
  },
