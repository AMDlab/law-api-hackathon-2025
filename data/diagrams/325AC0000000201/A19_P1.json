  "page_title": {
    "title": "敷地の衛生及び安全（高さ要件）",
    "target_subject": "建築物",
    "description": "建築物の敷地及び地盤面が、道の境及び周囲の土地より高いかを判定する"
  },

  "legal_ref": {
    "law_id": "325AC0000000201",
    "law_type": "act",
    "law_name": "建築基準法",
    "law_abbrev": "法",
    "article": "19",
    "paragraph": "1",
    "item": null
  },

  "labels": ["単体規定", "敷地", "衛生・安全"],

  "text_raw": "建築物の敷地は、これに接する道の境より高くなければならず、建築物の地盤面は、これに接する周囲の土地より高くなければならない。ただし、敷地内の排水に支障がない場合又は建築物の用途により防湿の必要がない場合においては、この限りでない。",

  "compliance_logic": {
    "scope_condition": {
      "operator": "ALWAYS",
      "value": true,
      "desc": "すべての建築物に適用"
    },
    "judgment_rule": {
      "operator": "AND_GLOBAL",
      "desc": "本文要件（敷地高さ AND 地盤面高さ）",
      "conditions": [
        {
          "id": "cond-001",
          "operator": "GT",
          "lhs": { "var": "site_height", "desc": "敷地の高さ", "property_type": "numeric" },
          "rhs": { "var": "road_boundary_height", "desc": "道の境の高さ", "property_type": "numeric" }
        },
        {
          "id": "cond-002",
          "operator": "GT",
          "lhs": { "var": "ground_level", "desc": "地盤面の高さ", "property_type": "numeric" },
          "rhs": { "var": "surrounding_land_height", "desc": "周囲の土地の高さ", "property_type": "numeric" }
        }
      ]
    },
    "exceptions": {
      "operator": "OR_GLOBAL",
      "effect": "exempt",
      "desc": "ただし、敷地内の排水に支障がない場合又は建築物の用途により防湿の必要がない場合においては、この限りでない",
      "items": [
        {
          "item_number": "1",
          "desc": "敷地内の排水に支障がない場合",
          "keywords": ["排水", "支障がない"],
          "note": "排水設備等により雨水等の排水が適切に行える場合"
        },
        {
          "item_number": "2",
          "desc": "建築物の用途により防湿の必要がない場合",
          "keywords": ["用途", "防湿", "必要がない"],
          "note": "車庫、倉庫等で湿気による支障が生じない用途",
          "examples": ["自動車車庫", "物置", "倉庫", "工場"]
        }
      ]
    }
  },

