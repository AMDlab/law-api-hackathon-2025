    "scope_condition": {
      "operator": "AND",
      "conditions": [
        {
          "type": "location",
          "value": "地区計画等が定められた区域内"
        }
      ],
      "note": "地区整備計画等が定められている区域"
    },
    "judgment_rule": {
      "operator": "AND",
