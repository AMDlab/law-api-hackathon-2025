{
  "id": "CHK-BSL-A21-P1",
  "version": "3.2.0",
  "page_title": {
    "title": "大規模建築物の主要構造部制限",
    "target_subject": "建築物",
    "description": "可燃材料を用いた大規模建築物の特定主要構造部が耐火性能を有するかを判定する"
  },
  "legal_ref": {
    "law_id": "325AC0000000201",
    "law_type": "act",
    "law_name": "建築基準法",
    "law_abbrev": "法",
    "article": "21",
    "paragraph": "1",
    "item": null
  },
  "labels": [
    "単体規定",
    "構造・防火",
    "耐火性能"
  ],
  "text_raw": "次の各号のいずれかに該当する建築物（その主要構造部（床、屋根及び階段を除く。）の政令で定める部分の全部又は一部に木材、プラスチックその他の可燃材料を用いたものに限る。）は、その特定主要構造部を通常火災終了時間（建築物の構造、建築設備及び用途に応じて通常の火災が消火の措置により終了するまでに通常要する時間をいう。）が経過するまでの間当該火災による建築物の倒壊及び延焼を防止するために特定主要構造部に必要とされる性能に関して政令で定める技術的基準に適合するもので、国土交通大臣が定めた構造方法を用いるもの又は国土交通大臣の認定を受けたものとしなければならない。ただし、その周囲に延焼防止上有効な空地で政令で定める技術的基準に適合するものを有する建築物については、この限りでない。",
  "compliance_logic": {
    "scope_condition": {
      "operator": "AND_GLOBAL",
      "desc": "適用対象の判定",
      "conditions": [
        {
          "operator": "EQ",
          "lhs": {
            "var": "uses_combustible_material",
            "desc": "主要構造部に可燃材料使用",
            "property_type": "proposition"
          },
          "rhs": {
            "value": true
          }
        },
        {
          "operator": "OR_GLOBAL",
          "desc": "次の各号のいずれかに該当",
          "conditions": [
            {
              "operator": "GTE",
              "lhs": { "var": "floors_above_ground", "desc": "地階を除く階数" },
              "rhs": { "value": 4, "desc": "4階" }
            },
            {
              "operator": "GT",
              "lhs": { "var": "building_height", "desc": "高さ" },
              "rhs": { "value": 16, "unit": "m", "desc": "16m" }
            },
            {
              "operator": "AND_LOCAL",
              "desc": "別表第一(五)項・(六)項の特殊建築物かつ高さ13m超",
              "conditions": [
                {
                  "operator": "IN",
                  "lhs": { "var": "building_use", "desc": "用途" },
                  "rhs": { "value": ["劇場", "映画館", "演芸場", "観覧場", "公会堂", "集会場", "病院", "診療所", "ホテル", "旅館", "下宿", "共同住宅", "寄宿舎", "児童福祉施設等"], "desc": "別表第一(五)項・(六)項用途" }
                },
                {
                  "operator": "GT",
                  "lhs": { "var": "building_height", "desc": "高さ" },
                  "rhs": { "value": 13, "unit": "m", "desc": "13m" }
                }
              ]
            }
          ]
        }
      ]
    },
    "judgment_rule": {
      "operator": "OR_GLOBAL",
      "desc": "次のいずれかを満たす",
      "conditions": [
        {
          "operator": "EQ",
          "lhs": { "var": "meets_fire_resistance_standard", "desc": "政令の技術的基準適合" },
          "rhs": { "value": true, "desc": "適合" }
        },
        {
          "operator": "EQ",
          "lhs": { "var": "has_effective_open_space", "desc": "延焼防止上有効な空地" },
          "rhs": { "value": true, "desc": "あり" }
        }
      ]
    },
    "exceptions": {
      "operator": "PROVISO",
      "effect": "exempt",
      "conditions": [
        {
          "operator": "EQ",
          "lhs": { "var": "has_effective_open_space", "desc": "延焼防止上有効な空地（政令基準適合）" },
          "rhs": { "value": true, "desc": "有する" }
        }
      ],
      "description": "周囲に延焼防止上有効な空地（政令基準適合）がある場合は適用除外"
    },
    "items": [
      {
        "item_number": "1",
        "desc": "地階を除く階数が4以上である建築物",
        "related_article": "法::A21:P1:I1",
        "keywords": ["4階以上", "地上階数"],
        "criteria": "地階を除く階数 >= 4"
      },
      {
        "item_number": "2",
        "desc": "高さが16mを超える建築物",
        "related_article": "法::A21:P1:I2",
        "keywords": ["高さ", "16m超"],
        "criteria": "建築物の高さ > 16m"
      },
      {
        "item_number": "3",
        "desc": "別表第一(五)項・(六)項の用途の特殊建築物で高さ13m超",
        "related_article": "法::A21:P1:I3",
        "keywords": ["劇場", "映画館", "演芸場", "観覧場", "公会堂", "集会場", "病院", "診療所", "ホテル", "旅館", "下宿", "共同住宅", "寄宿舎", "児童福祉施設等", "13m超"],
        "criteria": "用途が別表第一(五)項・(六)項に該当 AND 高さ > 13m",
        "note": "(五)項：劇場・映画館・演芸場・観覧場・公会堂・集会場等、(六)項：病院・診療所・児童福祉施設等・ホテル・旅館・下宿・共同住宅・寄宿舎等"
      }
    ]
  },
