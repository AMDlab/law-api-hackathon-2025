  "text_raw": "高さ三十一メートルをこえる建築物（政令で定めるものを除く。）には、非常用の昇降機を設けなければならない。",
  "compliance_logic": {
    "scope_condition": {
      "operator": "AND",
      "conditions": [
        {
          "type": "numeric_threshold",
          "property": "建築物の高さ",
          "operator": ">",
          "value": 31,
          "unit": "m"
        }
      ],
      "note": "政令で定める建築物を除く"
    },
    "judgment_rule": {
      "operator": "AND",
      "conditions": [
        {
          "ref": "info-height-exceeds",
          "operator": "==",
          "value": true
        },
        {
          "ref": "info-not-exempt",
          "operator": "==",
          "value": true
        },
        {
          "ref": "info-emergency-elevator-installed",
          "operator": "==",
          "value": true
        }
      ]
    },
    "exceptions": {
      "operator": "OR",
      "conditions": [
        {
          "type": "exemption",
          "description": "政令で定める建築物",
          "related_articles": ["令::A129_2"]
        }
      ],
      "effect": "EXEMPT"
    }
  },
