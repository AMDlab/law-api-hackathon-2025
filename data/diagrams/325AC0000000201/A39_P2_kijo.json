  "text_raw": "災害危険区域内における住居の用に供する建築物の建築の禁止その他建築物の建築に関する制限で災害防止上必要なものは、前項の条例で定める。",
  "compliance_logic": {
    "scope_condition": {
      "operator": "AND",
      "conditions": [
        { "type": "location", "value": "災害危険区域内" },
        { "type": "ordinance", "value": "地方公共団体が法39条1項に基づき制定した条例が存在" }
      ],
      "note": "敷地が災害危険区域内に位置する場合に適用。具体的な制限内容は地方公共団体の条例で定められる。"
    },
    "judgment_rule": {
      "operator": "AND",
      "conditions": [
        { "type": "ordinance_compliance", "ref": "info-restriction-compliance", "operator": "==", "value": true }
      ],
      "note": "地方公共団体の条例で定められた建築制限（住居用建築物の建築禁止その他災害防止上必要な制限）に適合すること"
    }
  },
