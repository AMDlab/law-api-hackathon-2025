  "page_title": {
    "title": "防火壁等",
    "target_subject": "建築物",
    "description": "延べ面積1000㎡超の建築物について、1000㎡以内ごとに防火壁または防火床で区画されているかを判定する"
  },
  "legal_ref": {
    "law_id": "325AC0000000201",
    "law_type": "act",
    "law_name": "建築基準法",
    "law_abbrev": "法",
