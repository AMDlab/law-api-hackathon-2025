      "conditions": [
        {
          "type": "location",
          "value": "地方公共団体が条例で地盤面を別に定めた区域内"
        }
      ],
      "note": "第52条第5項に基づき地方公共団体が条例で定める区域に限定"
    },
    "judgment_rule": {
      "operator": "EXISTS",
      "conditions": [
        {
          "type": "ordinance_existence",
          "description": "当該地方公共団体において第52条第5項に基づく条例が制定されているか"
        },
        {
          "type": "area_applicability",
          "description": "当該敷地が条例で定める区域内にあるか"
        }
      ]
    }
  },
