    "paragraph": "1",
    "item": null
  },

  "labels": ["単体規定", "防火", "外壁", "軒裏"],

  "text_raw": "延べ面積（同一敷地内に二以上の木造建築物等がある場合においては、その延べ面積の合計）が千平方メートルを超える木造建築物等は、その外壁及び軒裏で延焼のおそれのある部分を防火構造とし、その屋根の構造を第二十二条第一項に規定する構造としなければならない。",

  "compliance_logic": {
    "scope_condition": {
      "operator": "AND_GLOBAL",
      "desc": "適用対象の判定",
      "conditions": [
        {
          "id": "scope-wood",
          "operator": "EQ",
          "lhs": { "var": "is_wooden_building", "desc": "木造建築物等", "property_type": "proposition" },
          "rhs": { "value": true }
        },
        {
          "id": "scope-area",
          "operator": "GT",
          "lhs": { "var": "total_floor_area", "desc": "延べ面積（同一敷地内合計）", "property_type": "numeric" },
          "rhs": { "value": 1000, "unit": "㎡" }
        }
      ]
    },
    "judgment_rule": {
      "operator": "AND_GLOBAL",
      "desc": "外壁・軒裏・屋根の要件",
      "conditions": [
        {
          "id": "cond-001",
          "operator": "EQ",
          "lhs": { "var": "exterior_wall_fire_prevention", "desc": "外壁（延焼部分）が防火構造", "property_type": "proposition" },
          "rhs": { "value": true }
        },
        {
          "id": "cond-002",
          "operator": "EQ",
          "lhs": { "var": "eaves_fire_prevention", "desc": "軒裏（延焼部分）が防火構造", "property_type": "proposition" },
          "rhs": { "value": true }
        },
        {
          "id": "cond-003",
          "operator": "EQ",
          "lhs": { "var": "roof_article22_compliance", "desc": "屋根が法22条構造", "property_type": "proposition" },
          "rhs": { "value": true }
        }
      ]
    },
    "exceptions": null
  },

