        {
          "type": "relation",
          "value": "敷地が防災都市計画施設に接する"
        }
      ],
      "note": "第67条第6項の適用対象建築物"
    },
    "judgment_rule": {
      "operator": "AND",
      "conditions": [
        {
          "type": "height_comparison",
          "description": "建築物部分の高さが第6項に規定される最低限度より低いか"
        },
        {
          "type": "frontage_ratio",
          "description": "当該部分が防災都市計画施設に係る間口率の最低限度を超えているか"
        },
        {
          "type": "fireproof_structure",
          "description": "空隙のない壁など防火上有効な構造となっているか"
        }
      ]
    },
    "exceptions": null
