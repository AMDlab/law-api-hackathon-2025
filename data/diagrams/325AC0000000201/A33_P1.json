  "labels": ["単体規定", "設備", "避雷設備"],

  "text_raw": "高さ二十メートルをこえる建築物には、有効に避雷設備を設けなければならない。ただし、周囲の状況によつて安全上支障がない場合においては、この限りでない。",

  "compliance_logic": {
    "scope_condition": {
      "operator": "GT",
      "lhs": { "var": "building_height", "desc": "建築物の高さ", "property_type": "numeric" },
      "rhs": { "value": 20, "unit": "m" }
    },
    "judgment_rule": {
      "operator": "EQ",
      "lhs": { "var": "lightning_protection_installed", "desc": "有効に避雷設備を設けている", "property_type": "proposition" },
      "rhs": { "value": true }
    },
    "exceptions": {
      "operator": "EQ",
      "effect": "exempt",
      "desc": "ただし書き（周囲の状況によって安全上支障がない場合）",
      "items": [
        {
          "item_number": "1",
          "desc": "周囲の状況によって安全上支障がない場合",
          "keywords": ["周囲の状況", "安全上支障がない"]
        }
      ]
    }
  },

