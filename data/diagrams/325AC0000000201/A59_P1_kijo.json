{
  "id": "CHK-BSL-A59-P1",
  "version": "3.0.0",
  "page_title": {
    "title": "高度利用地区内の建築物の容積率等",
    "target_subject": "建築物",
    "description": "高度利用地区内の建築物の容積率、建蔽率及び建築面積が都市計画で定められた内容に適合することを判定する"
  },
  "legal_ref": {
    "law_id": "325AC0000000201",
    "law_type": "act",
    "law_name": "建築基準法",
    "law_abbrev": "法",
    "article": "59",
    "paragraph": "1",
    "item": null
  },
  "labels": [
    "集団規定",
    "高度利用地区"
  ],
  "text_raw": "高度利用地区内においては、建築物の容積率及び建蔽率並びに建築物の建築面積（同一敷地内に二以上の建築物がある場合においては、それぞれの建築面積）は、高度利用地区に関する都市計画において定められた内容に適合するものでなければならない。ただし、次の各号のいずれかに該当する建築物については、この限りでない。",
  "compliance_logic": {
    "scope_condition": {
      "operator": "AND",
      "conditions": [
        {
          "type": "location",
          "value": "高度利用地区内"
        }
      ],
      "note": "高度利用地区に関する都市計画で指定された区域"
    },
    "judgment_rule": {
      "operator": "OR",
      "conditions": [
        {
          "ref": "info-exempt",
          "operator": "==",
          "value": true
        },
        {
          "operator": "AND",
          "conditions": [
            {
              "ref": "info-volume-ratio-compliance",
              "operator": "==",
              "value": true
            },
            {
              "ref": "info-coverage-ratio-compliance",
              "operator": "==",
              "value": true
            },
            {
              "ref": "info-building-area-compliance",
              "operator": "==",
              "value": true
            }
          ]
        }
      ]
    },
    "exceptions": {
      "operator": "OR",
      "conditions": [
        {
          "note": "一号：主要構造部が木造等で階数2以下、地階なし、容易に移転・除却可能",
          "operator": "AND",
          "conditions": [
            {
              "ref": "info-main-structure",
              "value": "木造・鉄骨造・CB造等"
            },
            {
              "ref": "info-floors",
              "operator": "<=",
              "value": 2
            },
            {
              "ref": "info-has-basement",
              "operator": "==",
              "value": false
            },
            {
              "ref": "info-removable",
              "operator": "==",
              "value": true
            }
          ]
        },
        {
          "note": "二号：公衆便所、巡査派出所等で公益上必要なもの",
          "ref": "info-public-utility-type",
          "value": "公衆便所・巡査派出所等"
        },
        {
          "note": "三号：学校、駅舎、卸売市場等で特定行政庁許可",
          "operator": "AND",
          "conditions": [
            {
              "ref": "info-public-building-type",
              "value": "学校・駅舎・卸売市場等"
            },
            {
              "ref": "info-admin-permission",
              "operator": "==",
              "value": true
            }
          ]
        }
      ],
      "effect": "EXEMPT"
    }
  },
