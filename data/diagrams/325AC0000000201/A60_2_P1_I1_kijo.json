  "version": "3.0.0",
  "page_title": {
    "title": "都市再生特別地区内の建築物規制適合判定（臨時的小規模建築物除外判定）",
    "target_subject": "建築物",
    "description": "都市再生特別地区内に建築する建築物が、容積率・建蔽率・建築面積・高さの規制に適合するかを判定し、臨時的かつ容易に移転・除却できる小規模木造等建築物の場合は規制を除外する"
  },
  "legal_ref": {
    "law_id": "325AC0000000201",
    "law_type": "act",
    "law_name": "建築基準法",
    "law_abbrev": "法",
    "article": "60の2",
    "paragraph": "1",
    "item": "1"
  },
  "labels": [
    "単体規定",
    "都市計画関連",
    "容積率・建蔽率"
  ],
  "text_raw": "都市再生特別地区内においては、建築物の容積率及び建蔽率、建築物の建築面積（同一敷地内に二以上の建築物がある場合においては、それぞれの建築面積）並びに建築物の高さは、都市再生特別地区に関する都市計画において定められた内容に適合するものでなければならない。ただし、次の各号のいずれかに該当する建築物については、この限りでない。\n\n一　主要構造部が木造、鉄骨造、コンクリートブロック造その他これらに類する構造であつて、階数が二以下で、かつ、地階を有しない建築物で、容易に移転し、又は除却することができるもの",
  "compliance_logic": {
    "scope_condition": {
      "operator": "AND",
      "conditions": [
        {
          "type": "location",
          "value": "都市再生特別地区内"
        }
      ],
      "note": "都市計画において指定される都市再生特別地区"
    },
    "judgment_rule": {
      "operator": "OR",
      "conditions": [
        {
          "description": "主要要件を満たす場合",
          "operator": "AND",
          "conditions": [
            {
              "info_ref": "info-far",
              "condition": "都市計画で定められた容積率に適合"
            },
            {
              "info_ref": "info-bcr",
              "condition": "都市計画で定められた建蔽率に適合"
            },
            {
              "info_ref": "info-floor-area",
              "condition": "都市計画で定められた建築面積に適合"
            },
            {
              "info_ref": "info-height",
              "condition": "都市計画で定められた高さに適合"
            }
          ]
        },
        {
          "description": "I1の除外要件に該当する場合",
          "operator": "AND",
          "conditions": [
            {
              "info_ref": "info-structure-type",
              "condition": "木造、鉄骨造、コンクリートブロック造等に該当"
            },
            {
              "info_ref": "info-story-count",
              "condition": "階数が2以下"
            },
            {
              "info_ref": "info-basement",
              "condition": "地階を有さない"
            },
            {
              "info_ref": "info-removability",
              "condition": "容易に移転し、又は除却することができる"
            }
          ]
        }
      ]
    },
    "exceptions": {
      "operator": "OR",
      "conditions": [
        {
          "description": "I1の除外事由に該当",
          "condition": "主要構造部が木造等で、2階以下で地階なく、容易に移転・除却できる建築物"
        },
        {
          "description": "I2の除外事由に該当",
          "condition": "公衆便所、派出所等の公益上必要な建築物"
        },
        {
          "description": "I3の除外事由に該当",
          "condition": "学校、駅舎等で特定行政庁の許可を得た建築物"
        }
      ],
      "effect": "規制適用除外（容積率、建蔽率、建築面積、高さの制限不適用）"
    }
  },
