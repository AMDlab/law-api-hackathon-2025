  "text_raw": "第四十三条第一項第二号の道路の上空又は路面下に設ける建築物のうち、当該道路に係る地区計画の内容に適合し、かつ、政令で定める基準に適合するものであつて特定行政庁が安全上、防火上及び衛生上支障がないと認めるもの",
  "compliance_logic": {
    "scope_condition": {
      "operator": "AND",
      "conditions": [
        {
          "type": "prerequisite",
          "ref": "info-road-type-match",
          "note": "第43条1項2号の道路（地区計画区域内の道路）である"
        },
        {
          "type": "prerequisite",
          "ref": "info-position-match",
          "note": "当該道路の上空又は路面下に設ける建築物である"
        }
      ]
    },
    "judgment_rule": {
      "operator": "AND",
      "conditions": [
        {
          "ref": "info-district-plan-compliance",
          "note": "地区計画の内容に適合する"
        },
        {
          "ref": "info-order-standard-compliance",
          "note": "政令で定める基準に適合する"
        },
        {
          "ref": "info-admin-approval",
          "note": "特定行政庁が安全上、防火上及び衛生上支障がないと認める"
        }
      ]
    }
  },
